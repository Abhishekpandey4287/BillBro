<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude SharedPreferences that contain GCM registration ID -->
        <exclude domain="sharedpref" path="gcm.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Include everything but exclude temporary files -->
        <include domain="external" path="."/>
        <exclude domain="external" path="Android/sdk/"/>
        <exclude domain="root" path="."/>
        <exclude domain="file" path="."/>
        <exclude domain="database" path="."/>
    </device-transfer>
</data-extraction-rules>